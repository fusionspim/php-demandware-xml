{
  "name": "fusionspim/php-demandware-xml",
  "description": "PHP library for working with Demandware XML files",
  "keywords": ["demandware", "ecommerce", "export", "php", "xml"],
  "license": "MIT",
  "require": {
    "php": ">=7.2",
    "ext-json": "*",
    "ext-xmlwriter": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.3",
    "friendsofphp/php-cs-fixer": "^2.13"
  },
  "autoload": {
    "psr-4": { "DemandwareXml\\": "src" }
  },
  "autoload-dev": {
    "psr-4": { "DemandwareXml\\Test\\": "tests" }
  }
}
